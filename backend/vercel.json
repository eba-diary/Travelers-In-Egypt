{
  "version": 2,
  "builds": [
    {
      "src": "dist/app.js",
      "use": "@now/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ],
  "buildCommand": "npm run build && npx prisma generate"
}