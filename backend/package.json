{
  "name": "tie-backend",
  "version": "1.0.0",
  "description": "Travelers in Egypt backend with Koa and Redis",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "restart-db": "supabase db reset && npx prisma migrate dev && npm run generate-client && npm run seed",
    "seed": "ts-node prisma/seed.ts",
    "start": "npm run start-db && ts-node-dev src/app.ts",
    "start-db": "supabase start && npm run pull-migrations && npm run generate-client && npm run seed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx prisma migrate dev --name $1",
    "generate-client": "npx prisma generate --schema=./prisma/schema.prisma",
    "pull-migrations": "npx prisma migrate dev",
    "update-migrations": "npx prisma migrate dev"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "@supabase/supabase-js": "^2.24.0",
    "class-validator": "^0.14.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.1.3",
    "fast-csv": "^4.3.6",
    "fast-fuzzy": "^1.12.0",
    "fs": "^0.0.1-security",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.0",
    "koa-cors": "^0.0.16",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "redis": "^4.6.7",
    "rimraf": "^5.0.1",
    "supabase": "^1.163.6",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.6",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa2-cors": "^2.0.2",
    "@types/node": "^20.8.10",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-koa": "^2.0.2",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "prisma": "^5.12.1",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.4"
  }
}
