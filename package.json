{
  "devDependencies": {
    "concurrently": "^8.2.1",
    "prisma": "^5.2.0"
  },
  "scripts": {
    "start-apps": "concurrently \"npm run start-frontend\" \"npm run start-backend\" \" npm run start-supabase \" \" npm run\"",
    "start-frontend": "cd frontend/app && npm run dev",
    "start-backend": "cd backend && npm start",
    "start-supabase": "supabase stop && supabase start",
    "start-tools": "source tool-start.sh"
  }
}
